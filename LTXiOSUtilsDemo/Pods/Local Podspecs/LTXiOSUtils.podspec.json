{
  "name": "LTXiOSUtils",
  "version": "1.0.0.1",
  "platforms": {
    "ios": "10.0"
  },
  "summary": "通用工具类以及组件的整合、封装以及使用介绍",
  "homepage": "https://github.com/Coder-Star/LTXiOSUtils",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "CoderStar": "1340529758@qq.com"
  },
  "source": {
    "git": "local",
    "tag": "1.0.0.1"
  },
  "requires_arc": true,
  "swift_versions": [
    "5",
    "4.2"
  ],
  "subspecs": [
    {
      "name": "Tools",
      "subspecs": [
        {
          "name": "Extensions",
          "source_files": "LTXiOSUtils/Classes/Tools/Extensions/*.swift"
        },
        {
          "name": "Utils",
          "dependencies": {
            "LTXiOSUtils/Tools/Extensions": [

            ],
            "MBProgressHUD": [
              "1.1.0"
            ]
          },
          "source_files": "LTXiOSUtils/Classes/Tools/Utils/*.swift"
        },
        {
          "name": "Network",
          "dependencies": {
            "LTXiOSUtils/Tools/Utils": [

            ],
            "LTXiOSUtils/Tools/Extensions": [

            ],
            "Alamofire": [
              "4.9.1"
            ],
            "Moya": [
              "13.0.1"
            ],
            "SwiftyJSON": [
              "5.0.0"
            ]
          },
          "source_files": "LTXiOSUtils/Classes/Tools/Network/*.swift"
        }
      ]
    },
    {
      "name": "Constants",
      "source_files": "LTXiOSUtils/Classes/Constants/*.swift"
    },
    {
      "name": "Resources",
      "dependencies": {
        "Localize-Swift": [
          "3.1.0"
        ]
      },
      "source_files": "LTXiOSUtils/Resources/*.swift",
      "resource_bundles": {
        "LTXiOSUtils": "LTXiOSUtils/Resources/Resource/*"
      }
    },
    {
      "name": "Views",
      "dependencies": {
        "LTXiOSUtils/Resources": [

        ],
        "LTXiOSUtils/Constants": [

        ],
        "LTXiOSUtils/Tools/Extensions": [

        ]
      },
      "source_files": "LTXiOSUtils/Classes/Views/**/*.swift"
    },
    {
      "name": "ViewControllers",
      "dependencies": {
        "LTXiOSUtils/Constants": [

        ],
        "SnapKit": [
          "5.0.1"
        ]
      },
      "source_files": "LTXiOSUtils/Classes/ViewControllers/*.swift"
    }
  ],
  "swift_version": "5"
}
